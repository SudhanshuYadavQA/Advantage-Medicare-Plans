<!DOCTYPE suite
        SYSTEM "https://testng.org/testng-1.0.dtd">
<suite
        name="AdstiaSuite" parallel="tests" thread-count="3">
    <test
            name="ChromeTest">
        <parameter
                name="browser" value="chrome"/>
        <classes>
            <class
                    name="AdvantageMedicarePlans.validateGTM"/>
        </classes>
    </test>

    <test
            name="FirefoxTest">
        <parameter
                name="browser" value="firefox"/>
        <classes>
            <class
                    name="AdvantageMedicarePlans.validateGTM"/>
        </classes>
    </test>

    <test
            name="EdgeTest">
        <parameter
                name="browser" value="edge"/>
        <classes>
            <class
                    name="AdvantageMedicarePlans.validateGTM"/>
        </classes>
    </test>
</suite>